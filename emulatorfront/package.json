    "ace-builds": "^1.4.7",
    "axios": "^0.19.2",
    "babel-core": "^6.26.3",
    "electron-is-dev": "^1.1.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.0.6",
    "fs-jetpack": "^2.2.3",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-ace": "^8.0.0",
    "react-animated-css": "^1.2.1",
    "react-bulma-components": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-notifications-component": "^2.3.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
